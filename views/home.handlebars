<main>
	<a href="/plaats" class="button">Plaats een advertentie</a>
	<h1>Advertenties</h1>
	{{#each data}}
	<div>
		<h2>{{this.title}}</h2>
		<p>{{this.description}}</p>
		<iframe src="https://maps.google.com/maps?q={{this.location}}&z=13&output=embed" frameBorder="none" width="100%" height="300"></iframe>
	</div>
	{{/each}}
</main>